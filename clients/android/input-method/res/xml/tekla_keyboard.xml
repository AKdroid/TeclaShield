<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="16%p"
	android:horizontalGap="0px"
	android:verticalGap="0px"
	android:keyHeight="10%p"
	>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="19" android:keyIcon="@drawable/up"
			android:keyEdgeFlags="left" android:horizontalGap="2%p"/>
        <Key android:codes="20" android:keyIcon="@drawable/down"/>
        <Key android:codes="21" android:keyIcon="@drawable/left"/>
        <Key android:codes="22" android:keyIcon="@drawable/right"/>
        <Key android:codes="23" android:keyIcon="@drawable/enter"/>
        <Key android:codes="4" android:keyIcon="@drawable/back"/>
        <Key android:codes="-1" android:keyWidth="0px"
			android:keyEdgeFlags="right" android:horizontalGap="2%p"/>         		         		        		
    </Row>
    
</Keyboard>
    